<UserControl x:Class="DamoOneVision.Controls.Keypad.KoreanKeypad"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             Height="260" Width="330">
    <UniformGrid Columns="5" Rows="4" Margin="6">
        <Button Content="ㄱ" Click="OnBtn"/>
        <Button Content="ㄴ" Click="OnBtn"/>
        <Button Content="ㄷ" Click="OnBtn"/>
        <Button Content="ㄹ" Click="OnBtn"/>
        <Button Content="ㅁ" Click="OnBtn"/>
        <Button Content="ㅂ" Click="OnBtn"/>
        <Button Content="ㅅ" Click="OnBtn"/>
        <Button Content="ㅇ" Click="OnBtn"/>
        <Button Content="ㅈ" Click="OnBtn"/>
        <Button Content="ㅊ" Click="OnBtn"/>
        <Button Content="ㅌ" Click="OnBtn"/>
        <Button Content="ㅋ" Click="OnBtn"/>
        <Button Content="ㅍ" Click="OnBtn"/>
        <Button Content="ㅎ" Click="OnBtn"/>
        <Button Content="ㅏ" Click="OnBtn"/>
        <Button Content="ㅑ" Click="OnBtn"/>
        <Button Content="ㅓ" Click="OnBtn"/>
        <Button Content="ㅕ" Click="OnBtn"/>
        <Button Content="Space" Tag="Space" Click="OnBtn"/>
        <Button Content="OK" Tag="Done" Click="OnBtn"/>
    </UniformGrid>
</UserControl>
