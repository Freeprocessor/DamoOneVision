                    <Label Content="Binarized Threshold" HorizontalAlignment="Left" Margin="24,166,0,0" VerticalAlignment="Top"/>
                    <TextBox Text="Binarized Threshold" HorizontalAlignment="Left" Margin="149,171,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>

