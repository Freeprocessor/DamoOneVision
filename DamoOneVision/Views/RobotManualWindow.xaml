<Window x:Class="DamoOneVision.Views.RobotManualWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DamoOneVision.Views"
        mc:Ignorable="d"
        Title="RobotManualWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="RobotXAxisPositionTextBox"  HorizontalAlignment="Left" Margin="170,68,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="120" LostFocus="RobotXAxisPositionTextBox_LostFocus" />
        <Label x:Name="RobotXAxisPositionLabel" Content="Robot X-Axis" HorizontalAlignment="Left" Margin="50,64,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="RobotYAxisPositionTextBox" HorizontalAlignment="Left" Margin="170,95,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="120" LostFocus="RobotYAxisPositionTextBox_LostFocus" />
        <Label x:Name="RobotYAxisPositionLabel" Content="Robot Y-Axis" HorizontalAlignment="Left" Margin="50,91,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="RobotZAxisPositionTextBox" HorizontalAlignment="Left" Margin="170,122,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="120" LostFocus="RobotZAxisPositionTextBox_LostFocus" />
        <Label x:Name="RobotZAxisPositionLabel" Content="Robot Z-Axis" HorizontalAlignment="Left" Margin="50,118,0,0" VerticalAlignment="Top"/>
        <Button x:Name="RobotPickUpMoveButton" Content="Pick Up" HorizontalAlignment="Left" Margin="90,249,0,0" VerticalAlignment="Top" Height="37" Width="55" Click="RobotPickUpMoveButton_Click"/>
        <Button x:Name="RobotPlaceMoveButton" Content="Place" HorizontalAlignment="Left" Margin="202,249,0,0" VerticalAlignment="Top" Height="37" Width="56" Click="RobotPlaceMoveButton_Click"/>
        <Label x:Name="GV0Label" Content="0" HorizontalAlignment="Left" Margin="599,64,0,0" VerticalAlignment="Top"/>
        <Label x:Name="GV1Label" Content="0" HorizontalAlignment="Left" Margin="599,91,0,0" VerticalAlignment="Top"/>
        <Label x:Name="GV2Label" Content="0" HorizontalAlignment="Left" Margin="599,118,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
