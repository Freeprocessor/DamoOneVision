<Window x:Class="DamoOneVision.ManualWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DamoOneVision"
        mc:Ignorable="d"
        Title="ManualWindow" Height="525" Width="945">
    <Grid>
        <TextBox x:Name="ModbusIPTextBox" HorizontalAlignment="Left" Margin="129,51,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="ModbusIPlabel" Content="Modbus IP" HorizontalAlignment="Left" Margin="45,47,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="ModbusPortTextBox" HorizontalAlignment="Left" Margin="129,82,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="ModbusPortlabel" Content="Modbus Port" HorizontalAlignment="Left" Margin="45,78,0,0" VerticalAlignment="Top"/>
        <Button x:Name="ConnectButton" Content="Connect" HorizontalAlignment="Left" Margin="45,144,0,0" VerticalAlignment="Top" Height="60" Width="66" Click="ConnectButton_Click"/>
        <Button x:Name="DisconnectButton" Content="Disconnect" HorizontalAlignment="Left" Margin="183,144,0,0" VerticalAlignment="Top" Height="60" Width="66" Click="DisconnectButton_Click"/>

        <Rectangle x:Name="pcLifeBit" HorizontalAlignment="Left" Height="15" Width="15" Margin="90,274,0,0" Stroke="Black" Fill="Green" VerticalAlignment="Top" />
        <Label x:Name="PCLifeBitLabel" Content="PCLifeBit" HorizontalAlignment="Left" Margin="28,269,0,0" VerticalAlignment="Top"/>
        <Rectangle x:Name="plcLifeBit" HorizontalAlignment="Left" Height="15" Width="15" Margin="90,305,0,0" Stroke="Black" Fill="Green" VerticalAlignment="Top" />
        <Label x:Name="PLCLifeBitLabel" Content="PLCLifeBit" HorizontalAlignment="Left" Margin="28,300,0,0" VerticalAlignment="Top"/>

        <Button x:Name="TowerLampREDONButton" Content="Tower Lamp&#xA;RED ON Button" HorizontalAlignment="Left" Margin="305,31,0,0" VerticalAlignment="Top" Height="60" Width="105" Click="TowerLampREDONButton_MouseClick" Background="LightGray" />
        <Button x:Name="TowerLampREDOFFButton" Content="Tower Lamp&#xA;RED OFF Button" HorizontalAlignment="Left" Margin="440,31,0,0" VerticalAlignment="Top" Height="60" Width="105" Click="TowerLampREDOFFButton_MouseClick"/>
        <Button x:Name="TowerLampYELONButton" Content="Tower Lamp&#xA;YEL ON Button" HorizontalAlignment="Left" Margin="305,101,0,0" VerticalAlignment="Top" Height="60" Width="105" Click="TowerLampYELONButton_MouseClick" />
        <Button x:Name="TowerLampYELOFFButton" Content="Tower Lamp&#xA;YEL OFF Button" HorizontalAlignment="Left" Margin="440,101,0,0" VerticalAlignment="Top" Height="60" Width="105" Click="TowerLampYELOFFButton_MouseClick"/>
        <Button x:Name="TowerLampGENONButton" Content="Tower Lamp&#xA;GRN ON Button" HorizontalAlignment="Left" Margin="305,174,0,0" VerticalAlignment="Top" Height="60" Width="105" Click="TowerLampGRNONButton_MouseClick" />
        <Button x:Name="TowerLampGENOFFButton" Content="Tower Lamp&#xA;GRN OFF Button" HorizontalAlignment="Left" Margin="440,174,0,0" VerticalAlignment="Top" Height="60" Width="105" Click="TowerLampGRNOFFButton_MouseClick"/>
        <Button x:Name="EjectONButton" Content="Eject V/V&#xA;ON Button" HorizontalAlignment="Left" Margin="305,274,0,0" VerticalAlignment="Top" Height="60" Width="105" Click="EjectManualONButton_MouseClick" />
        <Button x:Name="EjectOFFButton" Content="Eject V/V&#xA;OFF Button" HorizontalAlignment="Left" Margin="440,274,0,0" VerticalAlignment="Top" Height="60" Width="105" Click="EjectManualOFFButton_MouseClick"/>
        <Button x:Name="MainCVONButton" Content="Main C/V&#xA;ON Button" HorizontalAlignment="Left" Margin="305,364,0,0" VerticalAlignment="Top" Height="60" Width="105" Click="MainCVONButton_MouseClick" />
        <Button x:Name="MainCVOFFButton" Content="Main C/V&#xA;OFF Button" HorizontalAlignment="Left" Margin="440,364,0,0" VerticalAlignment="Top" Height="60" Width="105" Click="MainCVOFFButton_MouseClick"/>
        <Button x:Name="Vision1LampONButton" Content="Vision - 1 Lamp&#xA;   ON Button" HorizontalAlignment="Left" Margin="625,31,0,0" VerticalAlignment="Top" Height="60" Width="105" Click="Vision1LampONButton_MouseClick" />
        <Button x:Name="Vision1LampOFFButton" Content="Vision - 1 Lamp&#xA;   OFF Button" HorizontalAlignment="Left" Margin="760,31,0,0" VerticalAlignment="Top" Height="60" Width="105" Click="Vision1LampOFFButton_MouseClick"/>
        <Button x:Name="Vision2LampONButton" Content="Vision - 2 Lamp&#xA;   ON Button" HorizontalAlignment="Left" Margin="625,101,0,0" VerticalAlignment="Top" Height="60" Width="105" Click="Vision2LampONButton_MouseClick" />
        <Button x:Name="Vision2LampOFFButton" Content="Vision - 2 Lamp&#xA;   OFF Button" HorizontalAlignment="Left" Margin="760,101,0,0" VerticalAlignment="Top" Height="60" Width="105" Click="Vision2LampOFFButton_MouseClick"/>
        <Button x:Name="Vision3LampONButton" Content="Vision - 3 Lamp&#xA;   ON Button" HorizontalAlignment="Left" Margin="625,174,0,0" VerticalAlignment="Top" Height="60" Width="105" Click="Vision3LampONButton_MouseClick" />
        <Button x:Name="Vision3LampOFFButton" Content="Vision - 3 Lamp&#xA;   OFF Button" HorizontalAlignment="Left" Margin="760,174,0,0" VerticalAlignment="Top" Height="60" Width="105" Click="Vision3LampOFFButton_MouseClick"/>
        <Button x:Name="EjectRoutineButton" Content="Eject V/V&#xA;Routine Button" HorizontalAlignment="Left" Margin="625,274,0,0" VerticalAlignment="Top" Height="60" Width="105" Click="EjectRoutineButton_MouseClick" Visibility="Hidden" />
    </Grid>
</Window>
